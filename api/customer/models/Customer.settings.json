    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "name": {
      "default": "",
      "type": "string"
    },
    "email": {
      "default": "",
      "type": "email"
    },
    "country": {
      "default": "",
      "type": "string"
    },
    "detail": {
      "default": "",
